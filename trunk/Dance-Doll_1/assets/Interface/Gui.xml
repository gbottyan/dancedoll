<!--home-->
                    <image id="home" filename="Interface/Button/home_normal.png" visibleToMouse="true" align="center" inset="1px" filter="true" >
                        <interact onClick="home()" />
                        <effect>
                            <onHover name="changeImage" active="Interface/Button/home_select.png" inactive="Interface/Button/home_normal.png" />
                            <onClick name="changeImage" active="Interface/Button/home_pressed.png" inactive="Interface/Button/home_normal.png" length="infinite" />
                        </effect>
                    </image>                 
                    
                    <panel align="left" width="100%" height="20px" ></panel>

